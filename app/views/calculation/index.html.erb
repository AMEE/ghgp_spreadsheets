<h1>Available Calculations</h1>
<%@available.each do |calc|%>
<%=link_to calc.name, :action=>'enter', :calculation=>calc.label%><br/>
<%end%>
<h1>Existing Calculations</h1>
<%@existing.each do |calc|%>
<%=link_to calc.profile_item_uid, :action=>'edit', 
  :calculation=>calc.label,
  :uid=>calc.profile_item_uid%><br/>
<%end%>
